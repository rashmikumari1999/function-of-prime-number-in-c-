#include <iostream>
#include<conio.h>
using namespace std;
int prime(int num);
int main()
{
    int num, res=0;
    cout<<"enter a number";
    cin>>num;
    res=prime(num);
    if(res==0)
    cout<<num<<"number is prime";
    else
    cout<<num<<"number is not prime";
}
int prime(int n)
{
    int i;
    for(i=2;i<=n/2;i++)
    {
        if(n%i!=0)
        continue;
        else
        return 1;
    }
    return 0;
}
